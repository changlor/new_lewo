<!--main-container-part-->
<div id="content">
<!--breadcrumbs-->
  <div id="content-header">
    <div id="breadcrumb"> <a href="index.html" title="返回首页" class="tip-bottom"><i class="icon-home"></i> 首页</a> <a>租客帐号管理</a> <a>帐号列表</a> <a class="current">合同信息</a></div>
  </div>
<!--End-breadcrumbs-->
<div class="container-fluid">
    <div class="widget-box">
        <div class="widget-title"> <span class="icon"> <i class="icon-align-justify"></i> </span>
          <h5>合同信息</h5>
          <a href="javascript:window.history.back(-1)" class="label label-info pull-left">返回</a>
        </div>
        <div class="widget-content nopadding">
          <form action="" method="post" class="form-horizontal form-inline">
            <div class="control-group">
              <label class="control-label">真实姓名 :</label>
              <div class="controls">
                {$contract_info.account_info.realname}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">电话 :</label>
              <div class="controls">
                {$contract_info.account_info.mobile}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">身份证 :</label>
              <div class="controls">
                {$contract_info.account_info.card_no}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">房间编码 :</label>
              <div class="controls">
                {$contract_info.room_info.room_code}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">押金 :</label>
              <div class="controls">
                {$contract_info.deposit}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">缴定金额 :</label>
              <div class="controls">
                {$contract_info.book_deposit}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">使用的余额 :</label>
              <div class="controls">
                {$contract_info.balance}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租期开始日 :</label>
              <div class="controls">
                {$contract_info.start_time}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租期结束日 :</label>
              <div class="controls">
                {$contract_info.end_time}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">实际退房日 :</label>
              <div class="controls">
                {$contract_info.actual_end_time}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租金 :</label>
              <div class="controls">
                {$contract_info.rent}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">服务费 :</label>
              <div class="controls">
                {$contract_info.fee}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">人数 :</label>
              <div class="controls">
                {$contract_info.person_count}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租期开始水表 :</label>
              <div class="controls">
                {$contract_info.zs}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租期开始电表 :</label>
              <div class="controls">
                {$contract_info.zd}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租期开始气表 :</label>
              <div class="controls">
                {$contract_info.zq}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">租期开始房间电表 :</label>
              <div class="controls">
                {$contract_info.roomd}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">合同总金额 :</label>
              <div class="controls">
                {$contract_info.total_fee}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">支付金额 :</label>
              <div class="controls">
                {$contract_info.pay_total}
              </div>
            </div>
            <div class="control-group">
              <label class="control-label">支付状态 :</label>
              <div class="controls">
                <if condition="$contract_info['pay_status'] eq 1">
                  <label class="label label-success">已支付</label>
                  <else/>
                  <label class="label label-important">未支付</label>
                </if>
              </div>
            </div>
             <div class="control-group">
              <label class="control-label">合同状态 :</label>
              <div class="controls">
                <switch name="contract_info.contract_status">
                  <case value="1">正常</case>
                  <case value="2">换房</case>
                  <case value="3">转租</case>
                  <case value="4">正常退房</case>
                  <case value="5">违约退房</case>
                </switch>
              </div>
            </div>
            <!-- <div class="form-actions">
              <button type="submit" class="btn btn-success">添加</button>
            </div> -->
          </form>
        </div>
      </div>
  </div>
</div>
<!--main-container-part-->
